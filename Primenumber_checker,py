num = int(input("Enter a number \n"))
Flag = False
if num == 1:
    print(f"{num}, not a prime number")
elif num > 1:
    for i in range(2,num):
        if (num % i == 0):
            Flag = True
            break
if Flag:
    print(f"{num}, not a prime number")
else:
    print(f"{num}, prime number")
    
